{
  "name": "mcts",
  "version": "0.1.0",
  "description": "Modern TypeScript Monte Carlo Tree Search library with zero-copy optimization",
  "repository": "https://github.com/dbravender/mcts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run build && mocha dist/test.js",
    "benchmark": "npm run build && node dist/benchmark.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcts",
    "monte-carlo",
    "tree-search",
    "game-ai",
    "ai",
    "algorithm",
    "typescript"
  ],
  "author": "Dan Bravender",
  "license": "MIT"
}
